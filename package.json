{
  "private": true,
  "name": "dashx",
  "packageManager": "yarn@1.22.18",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build --filter=@dashx/react --only",
    "dev": "turbo run dev --no-cache --continue",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules"
  },
  "devDependencies": {
    "eslint": "8.29.0",
    "turbo": "2.0.3"
  },
  "dependencies": {
    "eslint-config-turbo": "^2.0.9"
  }
}
